#include <stdio.h>

float square(float num) {
    return num * num;
}

int main() {
    float num, result;
    printf("Input any number for square: ");
    scanf("%f", &num);
    result = square(num);
    printf("The square of %.2f is: %.2f", num, result);
    return 0;
}
output:
Enter a number: 20
The square of 20.00 is : 400.00
