#include <stdio.h>

void even_odd(int num);

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);
    even_odd(num);
    return 0;
}

void even_odd(int num) {
    if (num % 2 == 0) {
        printf("The entered number is even.\n");
    } else {
        printf("The entered number is odd.\n");
    }
}
output:
Enter a number: 5
The entered number is odd.
